{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "linebreak-style": 0,
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true,
        "ignoreStrings": true,
        "ignoreUrls": true
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ]
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
